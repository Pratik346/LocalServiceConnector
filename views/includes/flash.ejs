  <%if(success && success.length){%>
        <div class="alert alert-success alert-dismissible fade show col-lg-6 col-md-8 col-sm-10 offset-3" role="alert"> <%=success%>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button></div>
    <%}%>
    <%if(error && error.length){%>
        <div class="alert alert-danger alert-dismissible fade show col-lg-6 col-md-8 col-sm-10 offset-3" role="alert"> <%=error%>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button></div>
    <%}%>